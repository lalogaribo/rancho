<% provide(:title, 'nutrientes') %>

<h1>Informacion Predios</h1>
<div class="row">
  <div class="col-md-12">
    <fieldset>
        <legend>Historial</legend>
        <div class="row">
            <div class="col-md-12">
             <table class="table table-striped">
              <tbody>
                <% @info_predios.each do |info_predio| %>
                  <tr>
                    <td align="center">Semana <%= info_predio.semana %></td>
                    <td><a rel="tooltip" title="Edit" class="btn btn-simple btn-warning btn-icon table-action edit" href="<%= edit_info_predio_path(info_predio) %>"><i class="glyphicon glyphicon-pencil"></i> </a></td>
                  </tr>
                <% end %>
              </tbody>
            </table>
            </div>
        </div>
    </fieldset>
 </div>
</div>
