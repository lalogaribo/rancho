<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= simple_form_for(@vuelo, html: { class: 'form-horizontal' }) do |f| %>
      <%= f.input :user_id %>
      <%= f.input :predio%>
      <%= f.input :aplicacion %>
      <%= f.input :piloto %>
      <%= f.input :precio_vuelo%>
      <%= f.button :submit, class: 'btn btn-primary btn-block' %>
    <% end %>
  </div>
  <div class="col-md-4 col-md-offset-5 ">
        [ <%= link_to 'Regresar a lista de vuelos', vuelos_path %>]
      </div>
</div>