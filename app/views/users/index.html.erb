<%= render 'shared/page_title', title: 'Showing all chefs' %>
<div class="text-center">
  <% @users.each do |user| %>
      <ul class="listing">
        <div class="row">
          <div class="well col-md-4 col-md-offset-4">
            <li><%= link_to gravatar_for(user), user_path(user) %></li>
            <li class="chef-title"><%= link_to user.name.capitalize, user_path(user) %></li>
            <%= link_to 'Eliminar record', user_path(user), method: :delete,
                        data: { confirm: 'Estas seguro de eliminar este usuario?' } %>
          </div>
        </div>
      </ul>
  <% end %>
  <%= link_to 'Dar de alta nuevo productor', signup_path %>
</div>